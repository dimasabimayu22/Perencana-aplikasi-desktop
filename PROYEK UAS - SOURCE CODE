login
String username = "dimas";
        String password = "123456";
        
        if(username.equalsIgnoreCase(jTextField1.getText())&& password.equalsIgnoreCase(jPasswordField1.getText())){
            this.setVisible(false);
            new hotel().show();
            this.dispose();
        }
        else {
            javax.swing.JOptionPane.showMessageDialog(null,"USER DAN SANDI YANG ANDA MASUKKAN SALAH !!");
            jTextField1.setText("");
            jPasswordField1.setText("");
            jTextField1.requestFocus();
        }
    }                                        

